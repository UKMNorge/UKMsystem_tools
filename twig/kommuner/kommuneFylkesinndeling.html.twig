<h2>Meldinger:</h2>

<div>
{% for key, kommune in log_meldinger %}
    <div class="card">
    <h4>{{ alle_kommuner[key].getNavn() }} {{ alle_kommuner[key].getId() }}</h4>
    {% for melding in kommune %}
        <p>{{ melding }}</p>
    {% endfor %}
    
    {% for err_melding in log_errors[key] %}
        <p style="padding: 2px; border: solid 1px orange">{{ err_melding }}</p>
    {% endfor %}

    {% for err_melding in log_criticals[key] %}
        <p style="padding: 2px; background: red; color: #fff">ERROR: {{ err_melding }}</p>
    {% endfor %}
    </div>
{% endfor %}
</div>

<h2>Kritiske feil</h2>
<div>
{% for key, kommune in log_meldinger %}
    {% for err_melding in log_criticals[key] %}
        <p style="padding: 2px; background: red; color: #fff">{{ err_melding }}</p>
    {% endfor %}
{% endfor %}
</div>

<hr>
<h2>Blogs p√• alle kommuner og fylker</h2>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Navn</th>
      <th scope="col">Success</th>
      <th scope="col">Melding</th>
    </tr>
  </thead>
  <tbody>
  {% for res in log_blogs %}
    <tr>
      <td>{{ res.navn }}</td>
      <td>{{ res.success ? 'ok' : 'feil' }}</td>
      <td>{{ res.msg }}</td>
    </tr>
{% endfor %}
  </tbody>
</table>